<h2>Users</h2>
<table>
  <thead>
  <th>image</th>
  <th>name</th>
  <th></th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr><td><%= attachment_image_tag user, :profile_image, :fill,60, 60, fallback: "no_image.jpg" %></td>
    <td><%= user.name %></td>
    <td><%= link_to "Show",book_path(@book) %></td>
    </tr>
  </tbody>
  
</table>